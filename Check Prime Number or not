class Solution
{
    
    static boolean checkprime(int a)
    {
        if(a == 0 || a == 1)
            return false;
        
        int m = a/2;
        
        for(int i=2;i<=m;i++)
        {
            if(a%i==0)
                return false;
        }
        
        return true;
        
    }
}
